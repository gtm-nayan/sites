		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "^1.0.0-next.55",
		"@sveltejs/site-kit": "workspace:*",
		"highlight.js": "^10.6.0",
		"marked": "^1.2.9",
		"prismjs": "^1.23.0",
		"svelte": "^3.35.0",
		"vite": "^2.1.2"
	},
	"type": "module"
}
